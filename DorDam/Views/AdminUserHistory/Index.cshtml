@model IEnumerable<DorDam_ENTITY.User_Info>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Common/Admin_Common_View.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-md-8 col-md-offset-2">
            <div class="panel panel-info">
                <div class="panel-heading">
                    <i class="fa fa-tags" aria-hidden="true"></i>&nbsp;<strong>Category</strong>
                </div>
                <div class="panel-body">
                    <table class="table table-striped">

                        @using (Html.BeginForm("showUserHistory", "AdminUserHistory"))
                        {
                            <tr>
                                <td align="center">
                                    <div class="form-group">
                                        <select class="form-control" name="UsernameDropDown">
                                            @foreach (var item in Model)
                                            {
                                                <option value="@item.User_name">@item.User_name</option>
                                            }
                                        </select>
                                    </div>
                                </td>
                            </tr>
                            <tr>
                                <td align="right"><input type="submit" class="btn btn-info" name="submitButton" value="Go" /></td>
                            </tr>
                        }
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@section Title{
    Admin
}


@section header{
    User Selection
}
